{
  "name": "yarsk-palmerev",
  "version": "1.0.0",
  "description": "My React boilerplate. Based on bradleyboy/yarsk.",
  "main": "./index.js",
  "scripts": {
    "lint:js": "eslint ./src ./conf ./build-gh-pages.js",
    "test": "mocha --compilers js:babel-core/register ./src/components/**/*.test.js",
    "start": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "build": "cross-env NODE_ENV=production webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist"
  },
  "author": "Evan Palmer <evjpalmer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-react-constant-elements": "^6.4.0",
    "babel-plugin-transform-react-inline-elements": "^6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gh-pages": "^0.9.0",
    "html-webpack-plugin": "^2.8.1",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.0",
    "style-loader": "^0.13.0",
